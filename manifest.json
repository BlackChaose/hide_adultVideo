{

  "description": "version 1.7.5 of parents control, block view on sites with adult content",
  "manifest_version": 2,
  "name": "hideAdultVideo",
  "version": "1.7.5",
  "homepage_url": "https://github.com/BlackChaose/hide_adultVideo",
  "icons": {
    "48": "icons/border-48.png"
  },
  "permissions": [
    "storage",
	"https://raw.githubusercontent.com/BlackChaose/*"
	],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["hideadultvideo.js"]
    }
  ]

}
